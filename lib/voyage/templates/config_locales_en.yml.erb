en:
  date:
    formats:
      default:
        '%m/%d/%Y'
      short:
        '%m/%d/%y'
      with_weekday:
        '%a %m/%d/%y'

  time:
    formats:
      default:
        '%a, %b %-d, %Y at %r'
      date:
        '%b %-d, %Y'
      short:
        '%B %d'
      time_with_period:
        '%-l:%M %p'
      date_and_time_with_period:
        '%m/%d/%Y - %-l:%M %p'

  titles:
    application: <%= app_name.humanize %>
